---
import Layout from "@/layouts/Layout.astro";
---

<Layout>
  <video controls width="640" height="360">
    <source src="/videos/cat.webm" type="video/webm" />

    <source src="/videos/cat.mp4" type="video/mp4" />

    <p>
      Your browser doesn't support HTML5 video. Here is a <a
        href="/videos/cat.mp4">link to the video</a
      > instead.
    </p>
  </video>
</Layout>
