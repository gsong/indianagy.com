{
  "name": "indianagy-dot-com",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "prebuild": "if [ \"$CLEAR_CACHE\" = \"true\" ]; then rm -rf node_modules/.astro; fi",
    "build": "astro build",
    "deploy": "netlify build && netlify deploy --prod",
    "dev": "astro dev",
    "postinstall": "is-ci || mise postinstall",
    "prepreview": "pnpm run build",
    "preview": "astro preview",
    "fix": "astro check && biome check --fix && prettier --write ."
  },
  "dependencies": {
    "@fontsource-variable/work-sans": "^5.2.6",
    "@fontsource/alice": "^5.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@web3forms/react": "^1.1.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.515.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.4.1",
    "@biomejs/biome": "^1.9.4",
    "@sindresorhus/slugify": "^2.2.1",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "astro": "^5.9.3",
    "is-ci": "^4.1.0",
    "netlify-cli": "^22.1.3",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-astro-organize-imports": "^0.4.11",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "sharp": "^0.34.2",
    "tailwindcss": "^4.1.10",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3"
  }
