{
  "name": "indianagy-dot-com",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "prebuild": "if [ \"$CLEAR_CACHE\" = \"true\" ]; then rm -rf node_modules/.astro; fi",
    "build": "astro build",
    "deploy": "netlify build && netlify deploy --prod",
    "dev": "astro dev",
    "postinstall": "is-ci || mise postinstall",
    "prepreview": "pnpm run build",
    "preview": "astro preview",
    "fix": "run-s fix:astro fix:biome fix:css fix:prettier",
    "fix:astro": "astro check",
    "fix:biome": "biome check --fix",
    "fix:css": "stylelint \"src/**/*.css\" --fix",
    "fix:prettier": "prettier --write .",
    "lint:css": "stylelint \"src/**/*.css\""
  },
  "dependencies": {
    "@fontsource-variable/work-sans": "^5.2.6",
    "@fontsource/alice": "^5.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@web3forms/react": "^1.1.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.541.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.5.1",
    "@biomejs/biome": "^2.2.2",
    "@sindresorhus/slugify": "^2.2.1",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "astro": "^5.13.3",
    "is-ci": "^4.1.0",
    "netlify-cli": "^23.3.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sharp": "^0.34.3",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2"
  }
}
