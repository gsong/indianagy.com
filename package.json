{
  "name": "indianagy-dot-com",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "prebuild": "if [ \"$CLEAR_CACHE\" = \"true\" ]; then rm -rf node_modules/.astro; fi",
    "build": "astro build",
    "deploy": "netlify build && netlify deploy --prod",
    "dev": "astro dev",
    "postinstall": "is-ci || mise postinstall",
    "prepreview": "pnpm run build",
    "preview": "astro preview",
    "fix": "run-s fix:astro fix:biome fix:prettier",
    "fix:astro": "astro check",
    "fix:biome": "biome check --fix",
    "fix:prettier": "prettier --write ."
  },
  "dependencies": {
    "@fontsource-variable/work-sans": "^5.2.8",
    "@fontsource/alice": "^5.2.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@web3forms/react": "^1.1.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.546.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.4.0",
    "@astrojs/sitemap": "^3.6.0",
    "@biomejs/biome": "^2.3.0",
    "@sindresorhus/slugify": "^3.0.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "astro": "^5.14.6",
    "is-ci": "^4.1.0",
    "netlify-cli": "^23.9.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.14",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3"
  }
}
